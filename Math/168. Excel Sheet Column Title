class Solution(object):
    def convertToTitle(self, columnNumber):
        """
        :type columnNumber: int
        :rtype: str
        """
        dict_a = {}
        result = ""
        while columnNumber > 0:
            columnNumber -= 1
            reminder = (columnNumber)%26
            result = chr(65+reminder) + result
            columnNumber //= 26
        return result
